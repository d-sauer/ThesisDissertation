<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:webproject2="http://www.netbeans.org/ns/web-project/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${java.home}/../lib/tools.jar:${j2ee.platform.wsimport.classpath}:${javac.classpath}"/>
        </taskdef>
        <condition property="conf-dir" value="${conf.dir}/" else="">
            <isset property="conf.dir"/>
        </condition>
    </target>
    <target name="wsimport-client-korisnici" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/korisnici"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/korisnici" destdir="${build.generated.dir}/jax-wsCache/korisnici" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/korisnici/wsdl/localhost_8443/dsauer_korisnici/korisnici.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="https://localhost:8443/dsauer_korisnici/korisnici?wsdl" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-service-references/korisnici/wsdl/localhost_8443/dsauer_korisnici/korisnici.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/korisnici"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/korisnici">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-korisnici" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/service"/>
        <delete dir="${build.generated.dir}/jax-wsCache/korisnici"/>
    </target>
    <target name="wsimport-client-prognoze" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/prognoze"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/prognoze" destdir="${build.generated.dir}/jax-wsCache/prognoze" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/prognoze/wsdl/localhost_8084/dsauer_prognoze/prognoze.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8084/dsauer_prognoze/prognoze?wsdl" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-service-references/prognoze/wsdl/localhost_8084/dsauer_prognoze/prognoze.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/prognoze"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/prognoze">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-prognoze" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/service"/>
        <delete dir="${build.generated.dir}/jax-wsCache/prognoze"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-korisnici, wsimport-client-prognoze"/>
</project>
